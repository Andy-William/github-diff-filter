{
  "manifest_version": 2,

  "name": "GitHub Diff Filter",
  "description": "A Chrome extension to allow you to filter the list of files in GitHub's comparison views.",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*/*/compare/*",
        "https://github.com/*/*/pull/*/files",
        "https://ghe.domain/*/*/compare/*",
        "https://ghe.domain/*/*/pull/*/files"
      ],
      "css": [
        "vendor/tipsy/tipsy.css",
        "app.css"
      ],
      "js": [
        "vendor/jquery/jquery.min.js",
        "vendor/tipsy/jquery.tipsy.js",
        "app.js"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "vendor/tipsy/tipsy.gif",
    "vendor/jquery/jquery.min.map"
  ]
}