{
  "manifest_version": 2,

  "name": "GitHub Diff Filter",
  "description": "A Chrome extension to allow you to filter the list of files in GitHub's comparison views.",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "ghe-url/*/*/compare/*",
        "ghe-url/*/*/pull/*/files"
      ],
      "css": [
        "bower_components/tipsy/src/stylesheets/tipsy.css",
        "app.css"
      ],
      "js": [
        "bower_components/jquery/dist/jquery.min.js",
        "bower_components/tipsy/src/javascripts/jquery.tipsy.js",
        "app.js"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "bower_components/tipsy/src/images/tipsy.gif",
    "bower_components/jquery/dist/jquery.min.map"
  ]
}